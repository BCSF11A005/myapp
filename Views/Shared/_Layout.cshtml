    <script>
        $(document).ready(function () {
            $("#flip").click(function () {
                $("#panel").slideToggle("slow");
            });
        });

        $(document).ready(function () {
            $("a").click(function () {

                var xyz = this.id;
                if (xyz == "ftaglayout") {
                    var fid = $(this).attr("value");





                    $.getJSON("ShowftagedfilesJSON?a=" + fid, function (data) {
                        if (data == null) {
                            alert("No data available");

                        }
                        else {





                            var htmlfiledetail = "";
                            for (var i in data) {

                                htmlfiledetail += "<tr><td><div>" + data[i].name + "</div><br /><div>" + data[i].virtualname + "</div></tr>";
                                htmlfiledetail += " <tr><td id =\"tablerowfiledetail\"colspan=\"3\"><div>" + data[i].url + "<img src=https://scontent-fra.xx.fbcdn.net/hphotos-xpa1/v/t1.0-9/19621_338629799669809_3752057226714125344_n.jpg?oh=07024173cec3b2832117d707215e4cb3&oe=55B5C77A></div></td></tr>";

                            }


                            $("#tablefiledetail").html(htmlfiledetail);

                            $("#filedetail").show();



                        }


                    });
                }


            });
        });
        $(document).ready(function () {
            $('#tagplus').click(function () {

                var value = $('#inp').val();



                $.getJSON("/User/addtagJSON?n=" + value, function (data) {

                    if (data == true) {
                        alert("Added");
                        window.location = "../User/Contacts";
                    }
                    else {
                        alert("Sorry Unable to addd");
                    }
                });

            });
        });


        $(document).ready(function () {
            $('#addctag').click(function () {
                $("#inp1").slideToggle("slow");
